{
  "valid": ["qidian", "qqac", "qqbook", "sfnovel", "u17", "comico",
    "omanhua", "2manhua", "57mh", "77mh", "yyls", "manhuatai"],
  "sources": {
    "manhuatai": {
      "url": "http://m.manhuatai.com/",
      "interceptor": [
        {
          "match": "^http://m.manhuatai.com/(\\w+)/\\d+\\w*\\.html",
          "goto": "readbook"
        }
      ],
      "remove": [".topBannerBox", ".footer"]
    },
    "omanhua": {
      "url": "http://m.omanhua.com/",
      "interceptor": [
        {
          "match": "^http://m.omanhua.com/comic/(\\d+)/\\d+/",
          "goto": "readbook"
        }
      ]
    },
    "2manhua": {
      "url": "http://m.2manhua.com",
      "interceptor": [
        {
          "match": "^http://m.2manhua.com/comic/(\\d+)/\\d+.html",
          "goto": "readbook"
        }
      ]
    },
    "57mh": {
      "url": "http://m.57mh.com",
      "interceptor": [
        {
          "match": "^http://m.57mh.com/(\\d+)/\\d+.html",
          "goto": "readbook"
        }
      ],
      "remove": [".module-mobile-cmt-barrage"]
    },
    "77mh": {
      "url": "http://m.77mh.com/",
      "interceptor": [
        {
          "match": "^http://m.77mh.com/colist_(\\d+).html",
          "goto": "bookdetail"
        }
      ]
    },
    "yyls": {
      "url": "http://8comic.se/",
      "interceptor": [
        {
          "match": "^http://8comic.se/(\\d+)/",
          "goto": "bookdetail"
        }
      ],
      "remove": [".sitemaji_banner"]
    },
    "sfnovel": {
      "url": "http://m.sfacg.com/",
      "interceptor": [
        {
          "match": "^http://m.sfacg.com/b/(\\d+)",
          "goto": "bookdetail"
        }
      ],
      "remove": ["#bottomNav"]
    },
    "qqac": {
      "url": "http://m.ac.qq.com/",
      "interceptor": [
        {
          "match": "block.html",
          "goto": {
            "type": "url",
            "target": "http://m.ac.qq.com/"
          }
        },
        {
          "match": "^http://m.ac.qq.com/chapter/index/id/(\\d+)/(cid|seqno)/\\d+",
          "execute": "window.stop()",
          "goto": {
            "type": "page",
            "target": "readbook"
          }
        }
      ],
      "remove": [".app-guiding"]
    },
    "qidian": {
      "url": "http://m.qidian.com/",
      "interceptor": [
        {
          "match": "^http://m.qidian.com/book/(\\d+)/\\d+",
          "goto": "readbook"
        }
      ],
      "remove": ["#asideTrigger", ".home-pure-ad", "#footerApp", ".footer-backtop", ".header-operate", ".guide-footer", ".guide-nav-a.jsLogin", ".mybook-game-ad"]
    },
    "u17": {
      "url": "http://m.u17.com",
      "interceptor": [
        {
          "match": "^http://m.u17.com/c/(\\d+).html",
          "goto": "bookdetail"
        }
      ],
      "remove": ["#ad"]
    },
    "comico": {
      "url": "http://www.comico.com.tw/spweb/index.nhn",
      "interceptor": [
        {
          "match": "^http://www.comico.com.tw/.*?titleNo=(\\d+)",
          "goto": "bookdetail"
        }
      ],
      "remove": ["#wrap > a", "#header > ul", "#footer > div"]
    },
    "chuangshi": {
      "url": "http://ubook.qq.com",
      "interceptor": [
        {
          "match": "^http://ubook.qq.com/8/read.html\\?bid=(\\d+).*",
          "goto": "readbook"
        }
      ]
    },
    "qqbook": {
      "url": "http://ubook.qq.com",
      "interceptor": [
        {
          "match": "^http://ubook.qq.com/8/read.html\\?bid=(\\d+).*",
          "goto": "readbook"
        }
      ]
    }
  }
}
