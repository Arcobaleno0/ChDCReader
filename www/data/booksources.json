              },
              "introduce": "pushName",
              "lastestChapter": "lastChapterName",
              "detailLink": ".book-mid-info>h4>a",
              "bookid": "id"
            }
        }
      },
      "detail": {
        "request": {
          "url": "http://ubook.3g.qq.com/8/intro?bid={bookid}",
          "type": "json",
          "timeout": 5
        },
        "response": {
          "author": "book::author",
          "catagory": "caname",
          "complete": {
            "element": "book::finished",
            "type": "boolean",
            "false": "0",
            "default": false
          },
          "coverImg": "book::cover",
          "introduce": "book::intro",
          "lastestChapter": "book::lastChapterName",
          "name": "book::title"
        }
      },
      "catalog": {
        "request": {
          "url": "http://ubook.3g.qq.com/8/chapter?bid={bookid}&pageNo={pageNo}",
          "type": "JSON"
        },
        "response": {
          "type": "array",
          "element": "chapter",
          "children": {
            "title": "title",
            "cid": {
              "type": "string",
              "element": "seq",
              "valid": "{seq} <= {maxfreechapter}"
            }
          }
        }
      },
      "chapter": {
        "request": {
          "url": "http://ubook.3g.qq.com/8/book/chapter?bid={bookid}&cid={cid}",
          "type": "JSON"
        },
        "response": {
          "contentHTML": "content",
          "modifyTime": "chapter::updateTime",
          "title": "chapter::title"
        }
      }
    },
