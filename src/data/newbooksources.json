{
    "qidian": {
        "id": "qidian",
        "name": "起点中文网",
        "contentSourceWeight": -10,
        "mainSourceWeight": 20,
        "host": "http://www.qidian.com/",
        "search": {
            "request": {
                "url": "http://se.qidian.com/?kw={keyword}",
                "timeout": 5
            },
            "response": {
                "type": "array",
                "element": "#result-list li[data-rid]",
                "children": {
                        "name": ".book-mid-info>h4>a",
                        "author": ".book-mid-info .author>a.name",
                        "catagory": ".book-mid-info .author a:nth-child(4)",
                        "complete": {
                            "type": "boolean",
                            "element": ".book-mid-info .author span:last-child",
                            "true": "完本",
                            "false": "连载中"
                        },
                        "coverImg": ".book-img-box img",
                        "introduce": ".book-mid-info .intro",
                        "lastestChapter": ".book-mid-info .update>a",
                        "detailLink": ".book-mid-info>h4>a",
                        "bookid": {
                            "type": "string",
                            "element": ".book-mid-info>h4>a",
                            "attribute": "data-bid"
                        }
                    }
            }
        },
        "detail": {
            "request": {
                "url": "{detailLink}",
                "timeout": 5
            },
            "response": {
                "author": "div.book-info > h1 > span > a",
                "catagory": "div.book-info > p.tag > a:nth-child(4)",
                "complete": "div.book-info > p.tag > span:nth-child(1)",
                "coverImg": "#bookImg > img",
                "introduce": "div.book-intro > p",
                "lastestChapter": "div.book-info-detail > div.book-state li.update a",
                "name": "div.book-info > h1 > em"
            }
        },
        "catalog": {
            "request": {
                "url": "http://book.qidian.com/ajax/book/category?_csrfToken={csrfToken}&bookId={bookid}",
                "timeout": 15,
                "type": "JSON"
            },
            "response": {
                "type": "array",
                "element": "data.vs.cs",
                "children": {
                    "name": "cN",
                    "linkid": "cU",
                    "vip": "sS",
                    "link": {
                        "type": "format",
                        "value": "http://read.qidian.com/chapter/{linkid}",
                        "valid": "{vip}==1"
                    }
                }
            }
        },
        "chapter": {
            "request": {
                "url": "{chapterLink}",
                "timeout": 5
            },
            "response": {
                "contentHTML": "div.read-content",
                "modifyTime": "div.text-info > div.info > i:nth-child(4) > span",
                "title": "div.text-head > h3"
            }
        }
    },
    "chuangshi": {
        "id": "chuangshi",
        "name": "创世中文网",
        "contentSourceWeight": -100,
        "mainSourceWeight": 10,
        "host": "http://chuangshi.qq.com/",
        "search": {
            "response": {
                "element": "#searchResultList > li",
                "detail": {
                    "author": "div.search_r_info > p:nth-child(2) > b > a",
                    "catagory": "div.search_r_info > p:nth-child(3) > a:nth-child(1)",
                    "complete": "div.search_r_img > p > i",
                    "cover": "div.search_r_img > a > img",
                    "introduce": "div.search_r_info > p.search_i > em",
                    "lastestChapter": "div.search_r_info > p:nth-child(5) > a",
                    "link": "div.search_r_info > h1 > a",
                    "name": "div.search_r_info > h1 > a"
                }
            },
            "request": "http://chuangshi.qq.com/search/searchindex/type/chuangshi/wd/{keyword}.html"
        },
        "detail": {
            "response": {
                "catalogLink": "div.mainbox > div.main1 > div.tablist > ul > li:nth-child(2) > a",
                "author": "div.mainbox > div.autherinfo > div.au_name > p:nth-child(2) > a",
                "catagory": "div.mainbox > div.main2 > div.left > div.title > a:nth-child(2)",
                "complete": "div.mainbox > div.main1 > div.title > i:nth-child(3)",
                "cover": "div.mainbox > div.main2 > div.left > div.cover > a > img",
                "introduce": "div.mainbox > div.main2 > div.left > div.info",
                "lastestChapter": "#newChapterList > div.chaptername > b > a",
                "name": "div.mainbox > div.main1 > div.title > a > b"
            }
        },
        "catalog": {
            "link": "{catalogLink}",
            "type": "html",
            "response": {
                "link": "div.indexbox ul > li > a"
            }
        },
        "chapter": {
            "response": {
                "content": ".bookreadercontent",
                "modifyTime": "#chaptercontainer > div > div.story_title > p > span:nth-child(3)",
                "title": "#chaptercontainer > div > div.story_title > h1"
            }
        }
    },

    "17k": {
        "id": "17k",
        "name": "17K",
        "contentSourceWeight": -100,
        "mainSourceWeight": 10,
        "host": "http://www.17k.com/",
        "search": {
            "response": {
                "element": ".search-list > .textlist",
                "detail": {
                    "author": "div.textmiddle > dl > dd > ul > li.bq > span.ls > a",
                    "catagory": "div.textmiddle > dl > dd > ul > li.bq > span:nth-child(2) > a",
                    "complete": "",
                    "cover": "div.textleft img",
                    "introduce": "div.textmiddle > dl > dd > ul > li:nth-child(3) > p > a",
                    "lastestChapter": "div.textmiddle > dl > dd > ul > li:nth-child(4) > a",
                    "link": "div.textmiddle > dl > dt > a",
                    "name": "div.textmiddle > dl > dt > a"
                }
            },
            "request": "http://search.17k.com/search.xhtml?c.st=0&c.q={keyword}"
        },
        "detail": {
            "response": {
                "catalogLink": "div.BookInfo > div.Props > dl > dt > a",
                "author": "div.AuthorInfo > div > a.name",
                "catagory": "body > div.infoPath > div > a:nth-child(3)",
                "complete": "div.BookInfo > div.Info > div.label > a:nth-child(1) > span",
                "cover": "div.BookInfo > div.Props > div > a > img",
                "introduce": "div.BookInfo > div.Info.Sign > dl > dd > div:nth-child(1) > a",
                "lastestChapter": "dl.NewsChapter > dd > ul > li:nth-child(2) > a",
                "name": "div.BookInfo > div.Info.Sign > h1 > a"
            }
        },
        "catalog": {
            "link": "{catalogLink}",
            "type": "html",
            "response": {
                "link": "body > div.Main.List > dl > dd > a"
            }
        },
        "chapter": {
            "response": {
                "content": "#chapterContentWapper",
                "modifyTime": "#readAreaBox > div.chapter_update_time",
                "title": "#readAreaBox > h1"
            }
        }
    },

