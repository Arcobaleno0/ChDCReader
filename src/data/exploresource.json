{
  "valid": ["qidian", "chuangshi", "sfnovel", "u17", "comico", "qqac"],
  "sources": {
    "sfnovel": {
      "url": "http://m.sfacg.com/",
      "bookdetail": {
        "matcher": "^http://m.sfacg.com/b/(\\d+)"
      }
    },
    "qqac": {
      "url": "http://m.ac.qq.com/",
      "insertCSS": "#asideTrigger, .home-pure-ad, #footerApp, .footer-backtop, .header-operate, .guide-footer, .guide-nav-a.jsLogin, .mybook-game-ad {display:none;}",
      "readbook": {
        "matcher": "^http://m.ac.qq.com/chapter/index/id/(\\d+)/(cid|seqno)/\\d+"
      }
    },
    "qidian": {
      "url": "http://m.qidian.com/",
      "insertCSS": "#asideTrigger, .home-pure-ad, #footerApp, .footer-backtop, .header-operate, .guide-footer, .guide-nav-a.jsLogin, .mybook-game-ad {display:none;}",
      "readbook": {
        "matcher": "^http://m.qidian.com/book/(\\d+)/\\d+"
      }
    },
    "u17": {
      "url": "http://m.u17.com",
      "insertCSS": "#ad {display: none;}",
      "bookdetail": {
        "matcher": "^http://m.u17.com/c/(\\d+).html"
      }
    },
    "comico": {
      "url": "http://www.comico.com.tw",
      "bookdetail": {
        "matcher": "^http://www.comico.com.tw/.*?titleNo=(\\d+)"
      }
    },
    "chuangshi": {
      "url": "http://ubook.qq.com",
      "readbook": {
        "matcher": "^http://ubook.qq.com/8/read.html\\?bid=(\\d+).*"
      }
    },
    "17k": {
      "url": "http://m.qidian.com/",
      "readbook": {
        "matcher": "^http://m.qidian.com/book/(\\d+)/\\d+.*"
      }
    }
  }
}
